    <validationRules>
        <fullName>Close_Date_Rule</fullName>
        <active>true</active>
        <description>Close Date must be specified when Status is set to &apos;Closed - Filled&apos; or &apos;Closed - Not Approved.&apos;</description>
        <errorConditionFormula>ISBLANK( Close_Date__c ) &amp;&amp; (  ISPICKVAL( Status__c , &quot;Closed - Filled&quot;) || ISPICKVAL( Status__c , &quot;Closed - Not Approved&quot;) )</errorConditionFormula>
        <errorDisplayField>Close_Date__c</errorDisplayField>
        <errorMessage>Close Date must be specified when Status is set to &apos;Closed.&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Max_Pay_Cannot_be_Empty</fullName>
        <active>true</active>
        <description>Maximum  pay cannot be empty</description>
        <errorConditionFormula>ISBLANK( Max_Pay__c )</errorConditionFormula>
        <errorDisplayField>Max_Pay__c</errorDisplayField>
        <errorMessage>Please Enter a Maximum Pay Scale for this position</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Min_Pay_Associate_Rule</fullName>
        <active>true</active>
        <description>Min pay for associate should be $1500</description>
        <errorConditionFormula>AND(Associat_Min_Pay__c   &gt;=  1500, Min_Pay__c  &lt; 1500)</errorConditionFormula>
        <errorDisplayField>Min_Pay__c</errorDisplayField>
        <errorMessage>Its an associate, Min_Pay should be $1500</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Min_Pay_Rule</fullName>
        <active>true</active>
        <description>Min Pay should never exceed Max Pay</description>
        <errorConditionFormula>Min_Pay__c &gt;  Max_Pay__c</errorConditionFormula>
        <errorDisplayField>Min_Pay__c</errorDisplayField>
        <errorMessage>Min Pay cannot exceed Max Pay</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Min_pay_cannot_be_blanked</fullName>
        <active>true</active>
        <description>Please enter the basic minimum pay</description>
        <errorConditionFormula>ISBLANK( Min_Pay__c )</errorConditionFormula>
        <errorDisplayField>Min_Pay__c</errorDisplayField>
        <errorMessage>Minimum Pay Cannot be Empty</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Move_Position_To_the_Position_Queue</fullName>
        <availability>online</availability>
        <description>Move position to the position queue</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Move_Position_To_the_Position_Queue</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/apex.js&quot;)}

var p_id = &apos;{!Position__c.Id}&apos;;

var resultSet = sforce.connection.query(&quot;SELECT id,Owner.id FROM Position__c WHERE Id = &apos;&quot; + p_id + &quot;&apos; limit 1&quot;);

var records = resultSet.getArray(&quot;records&quot;);

var pos_obj= records[0];
alert(&apos;{!$Label.Move_Position_To_the_Position_Queue}&apos;);
pos_obj.Ownerid = &apos;{!$Label.Move_Position_To_the_Position_Queue}&apos;;

var result = sforce.connection.update([pos_obj]);</url>
    </webLinks>
    <webLinks>
        <fullName>Open_Approved</fullName>
        <availability>online</availability>
        <description>Changes the position status to open-approved if the position status is new-no</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Open - Approved</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/apex.js&quot;)}

var p_id = &apos;{!Position__c.Id}&apos;;

var resultSet = sforce.connection.query(&quot;SELECT id,Approved_Date__c,status__c FROM Position__c WHERE Id = &apos;&quot; + p_id + &quot;&apos;&quot;);

var records = resultSet.getArray(&quot;records&quot;);
    
var pos_obj= records[0];  

var d =new Date();
var n=d.toJSON();

pos_obj.Approved_Date__c = n;

var result = sforce.connection.update([pos_obj]);</url>
